{
  "name": "tes",
  "version": "1.0.0",
  "description": "node js server for ci",
  "main": "server.js",
  "scripts": {
    "dev": "TS_NODE_FILES=1 nodemon src/server.ts -r ts-node/register",
    "build": "tsc -p tsconfig.server.json",
    "start": "NODE_ENV=production node dist/server.js",
    "test": "jest --rootDir __test__ --runInBand --detectOpenHandles --watch"
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development"
    },
    "ignore": [
      "builds/*",
      "cache/*.json"
    ]
  },
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoksS/Node-HW-SHRI.git"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "author": "MoksS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MoksS/Node-HW-SHRI/issues"
  },
  "homepage": "https://github.com/MoksS/Node-HW-SHRI#readme",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/nodegit": "^0.26.2",
    "axios": "^0.19.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "compression": "^1.7.4",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "nodegit": "^0.26.5",
    "nodemon": "^2.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-jest": "^25.4.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest": "^24.9.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  }
}
